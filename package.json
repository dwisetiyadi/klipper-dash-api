{
  "name": "klipper-dash-api",
  "version": "1.0.0",
  "description": "User interface API for Klipper 3D.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -e ts --exec \"tsc && node ./dist/index.js\"",
    "start": "tsc && pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "restart": "tsc && pm2 restart ecosystem.config.js",
    "postinstall": "node ./bin/postinstall.js && npm run precommit",
    "precommit": "rimraf .git/hooks/pre-commit.sample && rimraf .git/hooks/pre-commit && shx cp ./pre-commit .git/hooks/pre-commit && shx chmod +x .git/hooks/pre-commit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwisetiyadi/klipper-dash-api.git"
  },
  "keywords": [
    "3d",
    "printer",
    "klipper",
    "web",
    "touch",
    "ui"
  ],
  "author": "Dwi Setiyadi <dwi.setiyadi@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/dwisetiyadi/klipper-dash-api/issues"
  },
  "homepage": "https://github.com/dwisetiyadi/klipper-dash-api#readme",
  "dependencies": {
    "@hapi/boom": "9.0.0",
    "@hapi/hapi": "^19.0.5",
    "@hapi/inert": "^6.0.1",
    "axios": "^0.19.1",
    "class-validator": "^0.11.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "hapi-auth-jwt2": "^8.8.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "reflect-metadata": "^0.1.13",
    "serialport": "^8.0.6",
    "socket.io": "^2.3.0",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^8.0.1",
    "@types/hapi": "^18.0.3",
    "@types/hapi__boom": "^9.0.1",
    "@types/hapi__hapi": "^19.0.0",
    "@types/hapi__inert": "^5.2.0",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.8",
    "@types/serialport": "^8.0.0",
    "@types/socket.io": "^2.1.4",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@typescript-eslint/typescript-estree": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "glob": "^7.1.6",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.2.2",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "3.7.5"
  }
}
